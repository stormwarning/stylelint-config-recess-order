    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "semantic-release": "^15.13.0",
    "stylelint": "^9.9.0"
  },
  "keywords": [
    "bootstrap",
    "properties-order",
    "property order",
    "recess",
    "stylelint",
    "stylelint-config",
