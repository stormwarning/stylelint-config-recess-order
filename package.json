{
  "name": "stylelint-config-recess-order",
  "version": "3.1.0",
  "description": "Recess-based property sort order for Stylelint.",
  "keywords": [
    "bootstrap",
    "properties-order",
    "property order",
    "recess",
    "stylelint",
    "stylelint-config",
    "stylelint-order"
  ],
  "homepage": "https://github.com/stormwarning/stylelint-config-recess-order",
  "bugs": "https://github.com/stormwarning/stylelint-config-recess-order/issues",
  "repository": "stormwarning/stylelint-config-recess-order",
  "license": "ISC",
  "author": "Jeff (https://tidaltheory.io)",
  "main": "index.js",
  "files": [
    "index.js",
    "groups.js"
  ],
  "scripts": {
    "changeset": "changeset add",
    "lint": "eslint '**/*.js'",
    "prepare": "husky install",
    "release": "changeset publish",
    "test": "ava"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": "prettier --write"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "stylelint-order": "5.x"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.0",
    "@zazen/eslint-config": "6.2.1",
    "ava": "5.1.0",
    "eslint": "8.30.0",
    "esm": "3.2.25",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "prettier-plugin-packagejson": "2.3.0",
    "stylelint": "14.16.1"
  },
  "peerDependencies": {
    "stylelint": ">=14"
  }
}
