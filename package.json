{
  "name": "stylelint-config-recess-order",
  "version": "5.0.0",
  "description": "Recess-based property sort order for Stylelint.",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./groups": "./groups.js"
  },
  "keywords": [
    "bootstrap",
    "properties-order",
    "property order",
    "recess",
    "stylelint",
    "stylelint-config",
    "stylelint-order"
  ],
  "homepage": "https://github.com/stormwarning/stylelint-config-recess-order",
  "bugs": "https://github.com/stormwarning/stylelint-config-recess-order/issues",
  "repository": "stormwarning/stylelint-config-recess-order",
  "license": "ISC",
  "author": "Jeff (https://tidaltheory.io)",
  "main": "index.js",
  "files": [
    "index.js",
    "groups.js"
  ],
  "scripts": {
    "changeset": "changeset add",
    "lint": "eslint '**/*.js'",
    "prepare": "husky",
    "release": "changeset publish",
    "test": "ava"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": "prettier --write"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "stylelint-order": "^6.0.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@zazen/changesets-changelog": "2.0.3",
    "@zazen/eslint-config": "6.7.0",
    "ava": "5.2.0",
    "eslint": "8.56.0",
    "esm": "3.2.25",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.4.10",
    "stylelint": "16.2.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "stylelint": ">=16"
  }
}
